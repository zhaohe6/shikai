/* pages/main/main.wxss */
@font-face {
  font-family: 'iconfont';
  src: url('data:application/font-woff2;charset=utf-8;base64,') format('woff2'), url('data:application/font-woff;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-iconset0124:before {
  content: "\e617";
}
.icon-iconset0124 {
  font-size: 50rpx;
  font-weight: bolder;
  line-height: 100rpx;
}
.icon-fangzi:before {
  content: "\e8ff";
}
.icon-fangzi {
  font-size: 50rpx;
  font-weight: bolder;
  color: #5d9ae9;
}
.icon-shizhong:before {
  content: "\e61a";
}
.icon-shizhong {
  font-size: 40rpx;
  font-weight: bolder;
  line-height: 50rpx;
}
.icon-character:before {
  content: "\e62e";
}
.icon-xunzhang:before {
  content: "\e60e";
}
.icon-icon--:before {
  content: "\e734";
}
.icon-laba:before {
  content: "\e60c";
}
.infoBar {
  position: relative;
  width: 100%;
  background-color: #6eafff;
}
.avatar {
  display: inline-block;
  margin-top: 20px;
  margin-left: 10px;
}
open-data[type="userAvatarUrl"] {
  display: block;
  margin: 0px auto;
  background-color: #f9f9f9;
  width: 50px;
  height: 50px;
  overflow: hidden;
  border-radius: 50%;
}
open-data[type="userNickName"] {
  text-align: center;
  margin: 0 auto;
  display: block;
  color: aliceblue;
}
.day-title > .one {
  text-align: center;
  color: #ffffff;
  font-size: 36px;
}
.day-title > .two {
  text-align: center;
  color: #ffffff;
  font-size: 12px;
  margin-bottom: 30rpx;
}
.day-title > .btn {
  position: absolute;
  right: 10px;
  top: 30%;
  width: 80px;
  padding: 8px 0;
  font-size: 14px;
  background-color: #ffffff;
  border-radius: 20px;
  color: #6eafff;
  border: 1px solid #ffffff;
}
.object {
  width: 100%;
  padding: 30rpx 10rpx 10rpx 30rpx;
  display: flex;
  border-bottom: 1px dashed #bbb9b9;
}
.object > view {
  /* flex: 1; */
  padding: 0 20rpx;
  color: #5d9ae9;
  font-size: 27rpx;
}
.object > .btn {
  color: #5d9ae9;
  width: 80rpx;
  font-weight: 600;
  font-size: 13px;
  padding: 0 0;
  height: 40rpx;
  background-color: #c7e7fc;
  margin-top: 25rpx;
  border-radius: 20rpx;
}
.object > .img {
  /* flex:1; */
  width: 100rpx;
  height: 100rpx;
  background-color: #ededed;
  border-radius: 50%;
}
.object > .img {
  line-height: 100rpx;
  text-align: center;
}
.task {
  width: 100%;
}
.task > .bar {
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.task > .bar > text,
.task > .bar > navigator {
  /* flex: ; */
  color: #5d9ae9;
  font-size: 30rpx;
}
.content {
  display: flex;
  justify-content: center;
  padding: 30rpx 20rpx;
}
.content .img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #ededed;
  text-align: center;
}
.content view {
  padding: 0 20rpx;
}
.content navigator {
  text-align: center;
  color: #5d9ae9;
  font-size: 14px;
}
.content .text1 {
  color: #cbcbcb;
  font-size: 12px;
}
/* 日历样式 */
