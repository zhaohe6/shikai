.contain {
  padding: 20rpx;
  background-color: #cdf3ff;
}
.contain .user {
  background-color: #0093c4;
  border-radius: 30rpx;
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
}
.contain .user view {
  color: ghostwhite;
  font-weight: bolder;
}
.contain .user image {
  margin: 0rpx 20rpx;
  width: 30%;
  border-radius: 50%;
}
